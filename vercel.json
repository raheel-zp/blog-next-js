{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "includeFiles": "node_modules/.prisma/**"
    },
    "api/**/*.ts": {
      "includeFiles": "node_modules/.prisma/**"
    }
  },
  "env": {
    "DATABASE_URL": "@database_url"
  },
  "installCommand": "npm install",
  "buildCommand": "npx prisma generate && npm run build"
}
