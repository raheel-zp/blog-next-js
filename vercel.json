{
  "version": 2,
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "includeFiles": "node_modules/.prisma/**"
    },
    "api/**/*.ts": {
      "includeFiles": "node_modules/.prisma/**"
    }
  },
  "env": {
    "DATABASE_URL": "@database_url"
  },
  "installCommand": "npm install",
  "buildCommand": "npx prisma generate && npm run build",
  "ignoreCommand": "echo 'No ignored files'"
}
