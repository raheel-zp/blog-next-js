{
  "version": 2,
  "functions": {
    "app/api/**/*.ts": {
      "includeFiles": "node_modules/.prisma/**"
    },
    "app/api/**/*.js": {
      "includeFiles": "node_modules/.prisma/**"
    }
  },
  "installCommand": "npm install",
  "buildCommand": "npx prisma generate && npm run build",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ."
}
